{
  "name": "tuneOut",
  "description": "Tab specific audio stream music identification extension",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "./index-js/index-background.js"
    ]
  },
  "options_page": "./index-html/idx-options.html",
  "browser_action": {
    "default_popup": "./index-html/idx-popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ]
}